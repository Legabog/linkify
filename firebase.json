{
  "hosting": {
    "public": "./build",
    "i18n": {
      "root": "/locales"
    },
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "./storage.rules"
  }
}
